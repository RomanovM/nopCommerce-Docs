        <rule name="allotherpages" stopProcessing="true">
          <match url="^(pages/viewpage\.action.*)|(display.*)$" />
          <action type="Redirect" appendQueryString="false" url="/en/index.html" redirectType="Permanent" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
